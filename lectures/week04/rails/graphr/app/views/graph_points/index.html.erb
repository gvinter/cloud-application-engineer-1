<h1>Listing graph_points</h1>

<table>
  <tr>
    <th>X coord</th>
    <th>Y coord</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @graph_points.each do |graph_point| %>
  <tr>
    <td><%= graph_point.x_coord %></td>
    <td><%= graph_point.y_coord %></td>
    <td><%= link_to 'Show', graph_point %></td>
    <td><%= link_to 'Edit', edit_graph_point_path(graph_point) %></td>
    <td><%= link_to 'Destroy', graph_point, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Graph point', new_graph_point_path %>
